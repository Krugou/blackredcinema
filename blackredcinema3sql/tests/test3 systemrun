// Generated by Selenium IDE
const { Builder, By, Key, until } = require('selenium-webdriver')
const assert = require('assert')

describe('test run system', function() {
  this.timeout(30000)
  let driver
  let vars
  beforeEach(async function() {
    driver = await new Builder().forBrowser('chrome').build()
    vars = {}
  })
  afterEach(async function() {
    await driver.quit();
  })
  it('test run system', async function() {
    await driver.get("http://localhost:3000/")
    await driver.manage().window().setRect({ width: 1113, height: 1174 })
    await driver.findElement(By.linkText("continue to register your reservation.")).click()
    await driver.findElement(By.name("name")).click()
    await driver.findElement(By.name("name")).sendKeys("a2")
    await driver.findElement(By.css(".row:nth-child(1) > .col-25")).click()
    await driver.findElement(By.css("html")).click()
    await driver.findElement(By.name("email")).sendKeys("a2@gmail.com")
    await driver.findElement(By.name("password")).sendKeys("a2")
    await driver.findElement(By.name("password2")).sendKeys("a2")
    await driver.findElement(By.name("seating")).sendKeys("a2")
    await driver.findElement(By.id("movie")).click()
    {
      const dropdown = await driver.findElement(By.id("movie"))
      await dropdown.findElement(By.xpath("//option[. = 'Shawshank Redemption']")).click()
    }
    await driver.findElement(By.css("input:nth-child(3)")).click()
    await driver.findElement(By.name("email")).sendKeys("a2@gmail.com")
    await driver.findElement(By.name("password")).sendKeys("a2")
    await driver.findElement(By.css(".btn")).click()
    await driver.findElement(By.linkText("Logout")).click()
  })
})
